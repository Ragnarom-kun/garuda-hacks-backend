{
  "kind": "collectionType",
  "collectionName": "organization_users",
  "info": {
    "singularName": "organization-user",
    "pluralName": "organization-users",
    "displayName": "organization_user",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "email": {
      "type": "email",
      "required": true,
      "unique": true
    },
    "organization_name": {
      "type": "string",
      "required": true
    },
    "focus": {
      "type": "enumeration",
      "enum": [
        "IT/Tech",
        "Social",
        "Enterpreneurship",
        "Education"
      ],
      "required": true
    },
    "verified": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "password": {
      "type": "password",
      "required": true,
      "private": true
    },
    "niche": {
      "type": "date",
      "required": true
    },
    "contact": {
      "type": "biginteger"
    },
    "province": {
      "type": "string"
    },
    "city": {
      "type": "string"
    },
    "about": {
      "type": "text"
    }
  }
}
